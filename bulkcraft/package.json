{
  "name": "bulkcraft",
  "version": "0.1.0",
  "description": "AI-powered viral Twitter content generation and analysis tool",
  "main": "dist/index.js",
  "bin": {
    "bulkcraft": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "analyze": "tsx src/cli/index.ts analyze",
    "generate": "tsx src/cli/index.ts generate",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "twitter",
    "content-generation",
    "viral",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.4.0",
    "ora": "^8.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  }
}